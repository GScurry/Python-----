num=input('input a number: \n')
num=int(num)

def isprime(num):   
    count=int(num/2) 
    if num==1 or num==2:
        print('%s is a prime'%(num))
        return
    while count>1:
        print(count)
        print(num%count)
        if (num%count)==0:
            print('%s is not a prime'%(num))
            break
        count=count-1
    else:
        print('%s is a prime'%(num))  
      
isprime(num)      
      
